import smtplib
import ssl
from email.message import  EmailMessage
Email="mohitsshekhawat2006@gmail.com"
APP_PASSWORD="xxxxxxxxx"
RECEIVER="rohitssehkhawat4857923@gmail.com"
msg= EmailMessage()
msg.set_content("This email was shared by python code........")
content = ssl.create_default_context()
with smtplib.SMTP_SSL("smtp.gmail.com",456,context=content) as server:
    server.login(EMAIL, APP_PASSWORD)
    server.send_message(msg)
