pip install flask
from flask import Flask

app = Flask(__name__)

@app.route("/home")
def home():
    return """
    <h1> I am Home page </h1>
    <h2> I am running in flask </h2>
    """

@app.route("/about")
def abouta():
    return """
    <h1> I am home page </h1>
    <h1> I am running flask </h1>
    """

if __name__ == '__main__':
    app.run(debug=True)
